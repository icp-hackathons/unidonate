{
  "canisters": {
    "backend": {
      "type": "motoko",
      "main": "backend/main.mo",
      "post_install": ["echo 'Hello, World!'"]
    },
    "frontend": {
      "dependencies": ["backend"],
      "type": "assets",
      "source": ["build/"]
    }
  },
  "defaults": {
    "build": {
      "packtool": "npm run --silent sources"
    }
  },
  "output_env_file": ".env",
  "version": 1
}
